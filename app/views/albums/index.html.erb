 <h1>All Albums</h1>
 <hr>

 <table>
   <thead>
     <tr>
       <th colspan="3"></th>
     </tr>
   </thead>

   <tbody>
     <% @albums.each do |album| %>
       <tr>
         <td><%= link_to album.title, album, id: album.title %></td>
       </tr>
     <% end %>
   </tbody>
 </table>
