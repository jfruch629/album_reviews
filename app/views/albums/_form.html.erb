<%= form_for(@album) do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <br />
  <div class="field">
    <%= f.label :artist %><br />
    <%= f.text_field :artist %><br />
  </div><br />
  <div class="field">
    <%= f.label :summary %><br />
    <%= f.text_field :summary %>
  </div>
  <br />
  <div class="field">
    <%= f.label :starting_year %><br />
    <%= f.text_field :starting_year %>
  </div>
  <br />
  <div class="field">
    <%= f.label :genre %><br />
    <%= f.text_field :genre %>
  </div>
  <br />
  <div class="field">
    <%= f.label :tracks %><br />
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>
    <%= f.text_field :tracks, multiple: true %>

  </div>
  <br />
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
